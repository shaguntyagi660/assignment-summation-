import java.util.*;
class Swap_with{
public static void main(String arg[]){
int x,y,t;
Scanner sc =new Scanner(System.in);
System.out.println("Enter the value of X and Y");
x= sc.nextInt();
y= sc.nextInt();
System.out.println("before swapping numbers:");
t=x;
x=y;
y=t;
System.out.println("After swapping:"+x+" "+y);
System.out.println();
}
}
